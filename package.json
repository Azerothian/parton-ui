{
  "name": "@parton/ui",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "scripts": {
    "prepare": "husky",
    "build": "pnpm run pretty && pnpm run build-no-pretty",
    "build-no-pretty": "rm -Rf ./lib || true && tsc -b ",
    "lint": "pnpm run pretty && pnpm run lint-no-pretty",
    "lint-no-pretty": "eslint ./src/ --ext ts,tsx --report-unused-disable-directives",
    "pretty": "prettier . --write --ignore-unknown",
    "test": "pnpm run pretty && pnpm run test-no-pretty",
    "test-no-pretty": "vitest --run",
    "watch": "pnpm run pretty && vitest",
    "test-ui": "pnpm run pretty && vitest --ui",
    "prepublishOnly": "pnpm run pretty && pnpm run lint-no-pretty && pnpm run test-no-pretty && pnpm run build-no-pretty"
  },
  "dependencies": {
    "@apollo/client": "^3.10.8",
    "@vostro/clean-gql": "^1.0.15",
    "graphql": "^16.9.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.24.1"
  },
  "peerDependencies": {
    "@apollo/client": "^3.10.8",
    "@vostro/clean-gql": "^1.0.15",
    "graphql": "^16.9.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.24.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "@vitest/ui": "^2.0.3",
    "@vostro/jtd-types": "^1.0.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.8",
    "globby": "^14.0.2",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "memfs": "^4.9.3",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^2.0.3",
    "vitest-fetch-mock": "^0.3.0"
  }
}
